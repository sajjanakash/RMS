@model Project

@{ 
    ViewBag.Title = "Create Project Requirement";
}

<form asp-controller="Project" asp-action="Create" method="post" class="mt-3">
    <div class="form-group row">
        <label asp-for="ProjectName"></label>
        <input asp-for="ProjectName" />
    </div>

    <div class="form-group row">
        <label asp-for="ProjectCode"></label>
        <input asp-for="ProjectCode" />
    </div>

    <div class="form-group row">
        <label asp-for="Location"></label>
        <input asp-for="Location" />
    </div>

    <div class="form-group row">
        <label asp-for="ProjectType"></label>
        <select asp-for="ProjectType" asp-items="Html.GetEnumSelectList<ProjType>()"></select>
    </div>

    <div class="form-group row">
        <label asp-for="LastDate"></label>
        <input asp-for="LastDate" />
    </div>
    <div >
        <label asp-for="resource.SNo"></label>
        <label asp-for="resource.Designation"></label>
        <label asp-for="resource.SkillSet"></label>
        <label asp-for="resource.ExperienceInMonths"></label>
        <label asp-for="resource.NoOfCandidates"></label>
        
    </div>
    <div >
        <input asp-for="resource.SNo" />
        <input asp-for="resource.Designation" />
        <input asp-for="resource.SkillSet" />
        <input asp-for="resource.ExperienceInMonths" />
        <input asp-for="resource.NoOfCandidates" />
        
    </div>
    <div class="form-group row">
        <button type="submit">Post</button>
    </div>
</form>